<Window x:Class="Grindr.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Grindr"
        mc:Ignorable="d"
        Title="Grindr" Height="544" Width="858">
    <Grid>
        <TabControl x:Name="BotTabControl" HorizontalAlignment="Left" Height="450" Margin="22,44,0,0" VerticalAlignment="Top" Width="800">
            <TabItem Header="Summary">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
        <Button Content="Add Bot" HorizontalAlignment="Left" Margin="22,10,0,0" VerticalAlignment="Top" Width="75" Click="AddBotButton_Click"/>

    </Grid>
</Window>
